<h1>Music</h1>

<% if current_user %>
<div align="center"><%= link_to 'Add Music', new_tune_path %></div>
<% end %>

<div class="row">
  <div class="col-md-8">
    <% @tunes.each do |tune| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to tune.title, tune %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
  